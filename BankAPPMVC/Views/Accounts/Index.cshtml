@model IQueryable<BankPPP.Models.Account>
 

@{
    ViewBag.Title = "Accounts";
}

<h2>Accounts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
 
<table class="table">
    <tr>
        <th>
             Account Name 
        </th>
        <th>
            Balance
        </th>
       
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.accountname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.balance )
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { accountnumber = item.accountnumber }) |
                @Html.ActionLink("Details", "Details", new { accountnumber = item.accountnumber }) |
                @Html.ActionLink("Delete", "Delete", new { accountnumber = item.accountnumber })
            </td>
        </tr>
    }

</table>
 